{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "templateUrl": {
           "type": "string",
           "metadata": {
                "description": "Linked templates URL"
            }
        },
        "vnetTemplateName": {
           "type": "string",
           "metadata": {
                "description": "Vnet JSON linked template name"
            }
        }
    },
    "variables": {
        "clientName": "acme",
        "vnetName": "vnet01"
    },
    "resources": [
        {
            "name": "[variables('vnetName')]",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templateUrl'), '/linkedtemp/', parameters('vnetTemplateName'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "vnetNameOrig": "[concat('vnet', '-', variables('clientName'), '-', resourceGroup().location, '-', variables('vnetName'))]"
                }
            }
        }
    ],
    "outputs": {
        
    }
}